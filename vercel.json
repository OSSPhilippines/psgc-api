{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/"
        }
    ],
    "redirects": [
        { "source": "/region", "destination": "/api/region", "permanent": false },
        { "source": "/province", "destination": "/api/province", "permanent": false },
        { "source": "/city", "destination": "/api/city", "permanent": false },
        { "source": "/municipality", "destination": "/api/municipality", "permanent": false },
        { "source": "/barangay", "destination": "/api/barangay", "permanent": false },
        { "source": "/docs", "destination": "https://docs.wareneutron.com/psgc-api" }
    ],
    "regions": ["sin"],
    "cleanUrls": true
}
