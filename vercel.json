{
  "version": 2,
  "builds": [
      {
          "src": "src/index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/"
      }
  ],
  "env": {
    "MONGODB_URL": "@mongodb_url",
    "NODE_ENV": "production",
    "API_SECRET": ""
  }
}